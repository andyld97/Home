<Window x:Class="Home.Controls.Dialogs.ScreenshotDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Home.Controls.Dialogs" xmlns:controls="clr-namespace:Home.Controls"
        mc:Ignorable="d"
        xmlns:res="clr-namespace:Home.Properties"
        WindowState="Maximized"
        Title="{x:Static res:Resources.strScreenshotDialog_ShowScreenshot}" Height="450" Width="800">
    <Grid>
        <controls:ZoomImageViewer x:Name="ImageScreenshot" />
    </Grid>
</Window>
